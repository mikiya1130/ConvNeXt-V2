{
    "name": "ConvNeXt-V2",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "python",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "settings": {
                "files.eol": "\n",
                "files.insertFinalNewline": true,
                "files.trimFinalNewlines": true,
                "files.trimTrailingWhitespace": true,
                "[markdown]": {
                    "files.trimTrailingWhitespace": false
                },
                "files.exclude": {
                    "**/__pycache__": true
                },
                "files.watcherExclude": {
                    "**/__pycache__": true
                }
            },
            "extensions": [
                "ms-python.python"
            ]
        }
    }
}
